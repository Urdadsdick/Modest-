-- Check Game ID
local gameId = 12077443856 -- Target Game ID
if game.GameId ~= gameId then
    return print("This script is not for this game!")
end

-- Load Orion Library
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- Create Orion Window
local Window = OrionLib:MakeWindow({
    Name = "Kiddions Modest | Cali Shootout",
    HidePremium = false,
    SaveConfig = false,
    ConfigFolder = "KiddionsModest"
})

-- Notify User
OrionLib:MakeNotification({
    Name = "Loaded",
    Content = "Kiddions Modest | Cali Shootout script loaded successfully.",
    Image = "rbxassetid://4483345998", -- Optional Icon
    Time = 5
})

-- Initialize Orion Library
OrionLib:Init()
